{
    "name": "byjoby/html-object-strings",
    "description": "Abstraction layer for constructing arbitrary HTML tags and documents in PHP",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "myclabs/deep-copy": "^1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Joby Elliott",
            "email": "joby@byjoby.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ByJoby\\HTML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ByJoby\\HTML\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "stan": "phpstan"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "mustangostang/spyc": "^0.6.3"
    }
}
